/**
 * Definition for singly-linked list.
 * public class ListNode {
 *     int val;
 *     ListNode next;
 *     ListNode(int x) { val = x; }
 * }
 */
class Solution {
    public void deleteNode(ListNode p) {
        ListNode q = p.next;
        p.val = q.val;
        p.next = q.next;
      
        
    }
}
